# TourHub - Builder.io Enterprise Configuration
# Copy this file to .env and update the values for your environment

# Application Configuration
VITE_APP_NAME=TourHub
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION="Professional Tour Booking Platform"
VITE_APP_URL=https://tourhub.builder.io

# API Configuration
VITE_API_BASE_URL=https://dummyjson.com
VITE_API_TIMEOUT=10000

# Analytics Configuration
VITE_GOOGLE_ANALYTICS_ID=GA_TRACKING_ID
VITE_MIXPANEL_TOKEN=MIXPANEL_TOKEN

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Development Configuration
VITE_LOG_LEVEL=info
VITE_ENABLE_MOCK_API=false

# Builder.io Configuration (for future CMS integration)
VITE_BUILDER_IO_API_KEY=BUILDER_IO_API_KEY
VITE_BUILDER_IO_MODEL=page

# Social Media & SEO
VITE_SOCIAL_TWITTER=@builderio
VITE_SOCIAL_LINKEDIN=company/builder-io
VITE_SEO_KEYWORDS="tour booking, travel platform, builder.io"
